#include <16f887.h>
#use delay (clock=8000000)
#BYTE puertoA = 0x05
#include "timers.c"



void main(void)
{ 
  set_tris_a(0x00);
  timers_init(); 
  enable_interrupts(GLOBAL);
  
  timers_set_time(2,_100ms);
  
   
  while(1)
  {
    if(timers_get_time(2)== 0)
    {
       puertoA = puertoA ^ (1<<3);
       timers_set_time(2,_100ms);
    }
    
  }
}




